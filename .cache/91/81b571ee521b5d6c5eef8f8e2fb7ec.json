r\n\r\n/* global\r\n________________________________________________________________________________ */\r\n.hidden {\r\n\tdisplay: none !important;\r\n}\r\n\r\np, button {\r\n\tfont-size: 1rem;\r\n\tfont-family: var(--norm-font);\r\n\tcolor: var(--container-text-color);\r\n\tline-height: 1.5;\r\n\tbackground: transparent;\r\n}\r\n\r\np {\r\n\tmargin: 0;\r\n\ttext-align: center;\r\n\tword-break: break-word;\r\n\twidth: 100%;\r\n}\r\n\r\nbutton {\r\n\tbackground: var(--button-background-color);\r\n\tcolor: var(--button-text-color);\r\n}\r\n\r\n#calendar_container {\r\n\twidth: 90%;\r\n\tmax-width: 40rem;\r\n\tpadding: 10px 2.5%;\r\n\tpadding-bottom: 20px;\r\n\tborder-radius: 5px;\r\n\tmargin: 1rem auto;\r\n}\r\n\r\n#calendar_container p {\r\n\toverflow: hidden;\r\n\tword-break: keep-all;\r\n}\r\n\r\n.calendar_head {\r\n\tdisplay: flex;\r\n\talign-items: center;\r\n\t/* background: var(--container-background-color); */\r\n}\r\n\r\n.calendar_head > * {\r\n\tdisplay: inline-block;\r\n\tfont-family: var(--bold-font);\r\n}\r\n\r\n.calendar_head button {\r\n\tflex: 1;\r\n\tpadding: 0;\r\n\tbackground: transparent;\r\n}\r\n\r\n.calendar_head button p::before {\r\n\tdisplay: inline-block;\r\n\tfont-size: 3rem;\r\n\tline-height: 50%;\r\n}\r\n\r\n.calendar_head button:last-child p::before {\r\n\tcontent: \"›\";\r\n}\r\n\r\n.calendar_head button:first-child p::before {\r\n\tcontent: \"‹\";\r\n}\r\n\r\n.calendar_head #calendar_date {\r\n\tflex: 5;\r\n\tpadding: 5px 0;\r\n}\r\n\r\n.calendar_head #calendar_date p {\r\n\tfont-family: var(--bold-font);\r\n}\r\n\r\n.calendar_body .group {\r\n\tdisplay: inline-block;\r\n\twidth: calc(100% / 7);\r\n\tmargin: 0;\r\n}\r\n\r\n.calendar_body .group p {\r\n\tline-height: 2;\r\n\tfont-size: 0.75rem;\r\n\tpadding: 0;\r\n\tbox-sizing: border-box;\r\n\tcursor: default;\r\n\tborder: 0.001rem solid transparent;\r\n}\r\n\r\n.group .day_group p {\r\n\tcursor: pointer;\r\n\tborder-color: black;\r\n\tdisplay: inline-block;\r\n}\r\n\r\n.calendar_body t